public class Example {

	public static void main(String[] args) {

			final double PI = 3.14;

			double r, area;

			r = 10.2;

			area = r*r*PI;

			System.out.println("반지름 "+r+",");

			System.out.println("원의 면적 = "+ area);

			System.out.printf("원의 면적 = %.2f\n",area);

			byte k = (byte)300;

			

			int a = (int)10.2;

		

	}

}
