import java.util.Scanner;



public class ArrayEx {



	public static void main(String[] args) {

		Scanner sc = new Scanner (System.in);
		
		int n =0;
		int z =0;

		
		int arr []= new int [9];
		
		n=sc.nextInt();
		
		for (int i = 0; i<9; i++ ) {
			if (n != 0) {
			arr[i]=n%10;
			n /= 10;
			System.out.print(arr[i]+" ");
			z++;
			}
			else
				arr[i]= -1;
		}
		sc.nextLine();
		System.out.println();
		
		for (int i =z-1; i>=0; i--) {
			
			System.out.print(arr[i]+" ");
		
		}
		
	}
}
