import java.util.Scanner;

public class ForWhile2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n =0;
		int num =0;
		int z =0;
		System.out.print("정수 입력? ");
		n = sc.nextInt();
		num=n;
		System.out.print(n % 10+" ");
		
		while(true) {
			n/=10;
			z++;
			if (n<10)
				break;
			
		}

		if(n != 0){
		
		for (int i =0; i<z; i++) {
			num /=10;
			
			System.out.print(num%10+" ") ;
			
			
			}
		}
		
		
	}

}
